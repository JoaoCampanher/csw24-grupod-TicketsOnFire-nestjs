generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tenant {
  TenantID              String       @id @default(cuid())
  Nome                  String
  InformacoesDeContato  String
  ConfiguracoesEspecificas String
}

model Sample {
  SampleID              String       @id @default(uuid())
  Nome                  String
}